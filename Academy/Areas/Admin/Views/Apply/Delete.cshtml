@model Apply
@{
    ViewData["Title"] = "Delete";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<div class="mt-3">
    @Model.FirstName
</div>
<div class="mt-3">
    @Model.LastName
</div>
<div class="mt-3">
    @Model.EmailAddress
</div>
<div class="mt-3">
    @Model.TelNumber
</div>
<div class="mt-3">
    @Model.Message
</div>
<div class="row mt-3">
    <form method="post">
        <input type="submit" class="btn btn-danger ml-3"<i class="mdi mdi-delete"></i></input>
        <a class="btn btn-dark" asp-action="Index">Cancel</a>
    </form>
</div>