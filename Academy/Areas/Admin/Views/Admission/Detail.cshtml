@model Admissions
@{
    ViewData["Title"] = "Detail";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<div class="row">
    <img src="~/images/admissions/@Model.Image" width="400px" height="400px" />
</div>
<div>
    <h3 class="mt-3">Title</h3>
    @Model.Title
</div>
<div>
    <h3 class="mt-3">Description</h3>
    @Model.Description
</div>
<div>
    <h3 class="mt-3">Form</h3>
    @Model.Form
</div>
<div>
    <h3 class="mt-3">Card</h3>
    @Model.Card
</div>
<div>
    <h3 class="mt-3">Certificate</h3>
    @Model.Certificate
</div>
<div>
    <h3 class="mt-3">Transcript</h3>
    @Model.Transcript
</div>
<div>
    <h3 class="mt-3">Picture</h3>
    @Model.Picture
</div>
<div class="row mt-3">
    <a class="btn btn-dark" asp-action="Index">Go Back</a>
</div>
